- if @projects.any?
  %ul.projects-list
    - @projects.each do |project|
      %li.3d.projects-list-item
        %a.projects-list-link{href: '#'}
          .project-picture-container
            .project-picture{style: "background-image: url(#{project.featured_image.url})"}
            %h3.hover-title= project.title
            %p.hover-tags= project.description
- else
  %p.no-project.container No project yet.
